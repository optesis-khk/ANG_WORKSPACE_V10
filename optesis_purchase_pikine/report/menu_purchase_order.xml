<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="paperformat_modern_feelings_portrait" model="report.paperformat">
			<field name="name">Optesis purchase</field>
			<field name="default" eval="True" />
			<field name="page_width">0</field>
			<field name="orientation">Portrait</field>
			<field name="margin_top">10</field>
			<field name="margin_bottom">15</field>
			<field name="margin_left">0</field>
			<field name="margin_right">0</field>
			<field name="header_line" eval="True" />
			<field name="header_spacing">10</field>
			<field name="dpi">90</field>
		</record>

        <report
            string="Bon de commande Optesis"
            id="optesis_purchase_pikine.purchase_order"
            model="purchase.order"
            report_type="qweb-pdf"
            paperformat="optesis_purchase_pikine.paperformat_modern_feelings_portrait"
            name="optesis_purchase_pikine.report_purchase_order"
            file="optesis_purchase_pikine.report_purchase_order"
        />


    </data>
</odoo>
